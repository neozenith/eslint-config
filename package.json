{
  "name": "@neozenith/eslint-config",
  "version": "2.0.3",
  "description": "Shareable esLint config",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "npm run test:lint:fix",
    "test": "echo \"Error: no test specified\" && exit 0",
    "test:lint": "./node_modules/.bin/eslint --ignore-path .gitignore .",
    "test:lint:fix": "npm run test:lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neozenith/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Josh Peak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neozenith/eslint-config/issues"
  },
  "homepage": "https://github.com/neozenith/eslint-config#readme",
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "prettier": "^1.13.3"
  },
  "peerDependencies": {
    "eslint": ">= 4",
    "eslint-config-prettier": ">= 2",
    "eslint-plugin-prettier": ">= 2",
    "prettier": ">= 1"
  }
}
